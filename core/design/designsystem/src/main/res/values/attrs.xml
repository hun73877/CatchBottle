<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CBTextView">
        <attr name="cb_text" format="string" />
        <attr name="cb_textColor" format="integer" />
        <attr name="cb_textSize">
            <enum name="xs" value="0" />
            <enum name="s" value="1" />
            <enum name="m" value="2" />
            <enum name="l" value="3" />
            <enum name="xl" value="4" />
            <enum name="xxl" value="5" />
        </attr>

    </declare-styleable>

</resources>